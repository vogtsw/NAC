{
  "name": "nexus-agent-cluster",
  "version": "0.1.0",
  "description": "Multi-Agent Cluster Orchestration System with DAG-based parallel task scheduling",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "cli": "tsx src/cli.ts",
    "chat": "tsx src/cli.ts chat",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "format": "biome format --write .",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.0.4",
    "@fastify/websocket": "^11.2.0",
    "cron-parser": "^5.5.0",
    "dotenv": "^16.4.5",
    "fastify": "^5.7.4",
    "ioredis": "^5.4.1",
    "node-cron": "^4.2.1",
    "openai": "^4.73.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "ws": "^8.19.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.5",
    "@types/ws": "^8.18.1",
    "eslint": "^9.17.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
